<UserControl x:Class="GeorgeChen.MavenThought_VSExtension.MyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:MT="clr-namespace:GeorgeChen.MavenThought_VSExtension" mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
        <ItemsControl x:Name="itemscontrl">
            <ItemsControl.ItemTemplate>
                <DataTemplate >
                    <StackPanel>
                        <TextBlock Text="{Binding IsTest}"/>
                        <TextBlock Text="{Binding NameSpace}"/>
                        <TextBlock Text="{Binding Name}"/>
                    </StackPanel>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
</UserControl>
